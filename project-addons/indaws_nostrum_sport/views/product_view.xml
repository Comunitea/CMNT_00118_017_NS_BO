<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <!-- PRODUCTO -->
        <record id="view_nostrum_product_product_form" model="ir.ui.view">
            <field name="name">product.product.nostrum.form.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
            
                <field name="type" position="after">
                    <field name="url_presupuesto" widget="url" placeholder="url del producto..."/>
                </field>

            </field>
        </record>
</odoo>
