<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Days without shopping reminder -->
        <record id="dws_reminder_mail" model="ir.config_parameter">
            <field name="key">nostrum_dws.dws_reminder_mail</field>
